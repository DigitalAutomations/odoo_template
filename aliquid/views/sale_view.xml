<odoo>
    <record id="wizard_product_template_action" model="ir.actions.act_window">
        <field name="name">Import Prodotti</field>
        <field name="type">ir.actions.act_window</field>
        <field name="res_model">wizard.xls.product.template</field>
        <field name="view_id" ref="addoons_aliquid.wizard_import_product_template_form"/>
        <field name="target">new</field>
    </record>

    <record id="wizard_res_partner_action" model="ir.actions.act_window">
        <field name="name">Import Clienti/Fornitori</field>
        <field name="type">ir.actions.act_window</field>
        <field name="res_model">wizard.xls.res.partner</field>
        <field name="view_id" ref="addoons_aliquid.wizard_import_res_partner_form"/>
        <field name="target">new</field>
    </record>

    <menuitem id="aliquid_import_xls" name="Import Anagrafiche(XLSX)" sequence="100" parent="sale.menu_sale_config"/>
    <menuitem id="aliquid_import_products_xls" name="Prodotti" sequence="100"
              parent="addoons_aliquid.aliquid_import_xls" action="addoons_aliquid.wizard_product_template_action"/>
    <menuitem id="aliquid_import_partenrs_xls" name="Clienti" sequence="100"
              parent="addoons_aliquid.aliquid_import_xls" action="addoons_aliquid.wizard_res_partner_action"/>
</odoo>