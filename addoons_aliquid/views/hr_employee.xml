<odoo>
    <record id="aliquid_hr_employee_form" model="ir.ui.view">
        <field name="name">aliquid_hr_employee_form</field>
        <field name="model">hr.employee</field>
        <field name="inherit_id" ref="hr.view_employee_form"/>
        <field name="type">form</field>
        <field name="arch" type="xml">
            <page name="hr_settings" position="attributes">
                <attribute name="groups">addoons_aliquid.group_aliquid_enable_hr_configuration</attribute>
            </page>
            <group name="timesheet" position="attributes">
                <attribute name="groups">addoons_aliquid.group_aliquid_enable_costo_orario</attribute>
            </group>
        </field>
    </record>
</odoo>